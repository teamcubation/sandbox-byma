<configuration>
    <appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{36} - %msg%n - alalala</pattern>
        </encoder>
    </appender>
    <root>
        <appender-ref ref="CONSOLE"/>
    </root>
</configuration>